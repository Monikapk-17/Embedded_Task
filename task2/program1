// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n,i;
    printf("Enter the Elements :\t");
    scanf("%d",&n);
    int a[n];
    printf("Enter the numbers:\n");
    for (i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    int evensum=0;
    int oddsum=0;
    for (i=0;i<n;i++){
        if(a[i]%2==0)
        evensum +=a[i];
        else{
            oddsum +=a[i];
        }
    }
    printf("sum of even :%d\n",evensum);
    printf("sum of odd :%d\n",oddsum);
}
