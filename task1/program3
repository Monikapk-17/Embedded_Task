#include <stdio.h>

int main() {
    int n,i,j;
    printf("Enter the number: ");
    scanf("%d", &n);

    int matrix[n][n];

    for (i = 1; i <=n; i++) {
        for (j = 1; j<=n; j++) {
            matrix[i][j] = i + j;
        }
    }
    for (i = 1; i<=n; i++) {
        for (j = 1; j<=n; j++) {
            printf("%d ", matrix[i][j]);
        }
        printf("\n");
    }

    return 0;
}
